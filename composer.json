{
  "name": "weerd/example-package",
  "description": "PHP Composer package starter example with some common default files and directories.",
  "keywords": ["example", "keyword"],
  "license": "MIT",
  "authors": [
    {
      "name": "Diego Lorenzo",
      "email": "diego@diegolorenzo.com"
    }
  ],
  "require": {
    "php": ">=7.0.0"
  },
  "require-dev": {
    "illuminate/database": "~5.5",
    "symfony/var-dumper": "~4.0",
    "phpunit/phpunit": "~6.0"
  },
  "autoload": {
    "psr-4": {
      "Weerd\\ExamplePackage\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/TestCase.php"
    ],
    "psr-4": {
      "Weerd\\ExamplePackageTests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
